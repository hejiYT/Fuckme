  compatible = "pwm-backlight";
  pwms = <&pmc8180c_lpg 4 1000000>;
  enable-gpios = <&pmc8180c_gpios 8 0>;

  pinctrl-0 = <&bl_pwm_default>;
  pinctrl-names = "default";
 };

