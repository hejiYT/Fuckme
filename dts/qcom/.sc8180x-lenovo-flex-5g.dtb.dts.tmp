    };

    port@1 {
     reg = <1>;

     pmic_glink_con1_ss: endpoint {
      remote-endpoint = <&usb_sec_qmpphy_out>;
     };
    };

    port@2 {
     reg = <2>;

     pmic_glink_con1_sbu: endpoint {
      remote-endpoint = <&usbsec_sbu_mux>;
     };
    };
   };
  };
 };

