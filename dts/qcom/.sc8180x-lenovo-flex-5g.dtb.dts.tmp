  compatible = "gpio-keys";

  pinctrl-0 = <&hall_int_active_state>;
  pinctrl-names = "default";

  lid-switch {
   gpios = <&tlmm 121 1>;
   linux,input-type = <0x05>;
   linux,code = <0x00>;
   wakeup-source;
   wakeup-event-action = <0x02>;
  };
 };

